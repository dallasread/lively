{{>accounts-bar}}

<form>
    <div class="field">
        <label>Email</label>
        <input type="email" name="message">
    </div>
    <div class="field">
        <label>Password</label>
        <input type="password" name="password">
    </div>
    <div class="field">
        <label>Confirm Password</label>
        <input type="password">
    </div>
    <div class="action">
        <button class="btn btn-large btn-blue">
            Save Account
        </button>
    </div>
</form>
